{
    "name": "armincms/store",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "armincms/currency": "~0.2",
        "armincms/many-to-many": "~0.2", 
        "armincms/armincms": "~0.10",
        "armincms/orderable": "~0.1",
        "armincms/categorizable": "~1.2",
        "armincms/taxation": "*",
        "davidpiesse/nova-toggle": "~0.0",
        "epartment/nova-dependency-container": "~1.3",
        "whitecube/nova-flexible-content": "~0.1",
        "zareismail/nova-cascade": "~0.1",
        "optimistdigital/nova-multiselect-field": "~1.0",
        "ynacorp/nova-swatches": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Store\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Store\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
